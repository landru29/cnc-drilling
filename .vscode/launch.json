    "version": "0.2.0",
    "configurations": [
        {
            "name": "Info",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": [
                "info",
                "../testdata/point01.dxf"
            ]
        },
        {
            "name": "Drill",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": [
                "drill",
                "-d", "10.0",
                "--deep-per-try", "3",
                "-f", "320",
                "-o", "(100, 100)",
                "../testdata/point01.dxf"
            ]
        },
        {
            "name": "Engrave rectangle",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": [
                "engrave",
                "-d", "10.0",
                "--deep-per-try", "3",
                "../testdata/rectangle.dxf"
            ]
        },
        {
            "name": "Engrave 2 blocs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": [
                "engrave",
                "-d", "10.0",
                "--deep-per-try", "5",
                "../testdata/point01.dxf"
            ]
        },
        {
            "name": "Engrave polyline",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": [
                "engrave",
                "../testdata/polyline.dxf"
            ]
        },
        {
            "name": "Engrave circle",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd",
            "args": [
                "engrave",
                "../testdata/arc.dxf"
            ]
        },
    ]
}